void bubbleSort(vector<int>& arr, int n)
{ 
    for (int i = 1; i<n;i++){
        bool stop = false;
       for(int j = 0;j<n-i;j++){
           stop = true;
          if(arr[j]>arr[j+1]){
             swap(arr[j+1],arr[j]);
          }
       } 
        if(stop == false){
           break;
        }
    }
}
